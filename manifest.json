{
  "manifest_version": 3,
  "name": "Celia Token Miner",
  "version": "1.1",
  "background": {
    "service_worker": "background.js"
  },
  "description": "Automates token mining in Celia app.",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "alarms"
  ],
  "host_permissions": [
    "https://celia.finance/*",
    "https://app.celia.finance/app/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16-black.png",
      "48": "images/icon48-black.png",
      "128": "images/icon128-black.png"
    }
  }
}